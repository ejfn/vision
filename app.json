{
  "expo": {
    "name": "AiVision",
    "description": "An AI Vision app powered by Microsoft Cognitive Service",
    "slug": "vision",
    "privacy": "public",
    "version": "1.7.0",
    "githubUrl": "https://github.com/ericvan76/vision",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/emotions/*.png",
      "assets/spring.jpg"
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.fanstek.vision",
      "buildNumber": "1070008",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Please allow AiVision to use your camera to take photos for cognitive analysis.",
        "NSPhotoLibraryUsageDescription": "Pleas allow AiVision to access your photos for cognitive analysis.",
        "NSPhotoLibraryAddUsageDescription": "Please allow AiVision to save photos."
      },
      "loadJSInBackgroundExperimental": true
    },
    "android": {
      "package": "com.fanstek.vision",
      "versionCode": 1070008,
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/adaptive-icon-background.png"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
