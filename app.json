{
  "expo": {
    "name": "AiVision",
    "description": "An AI Vision app powered by Microsoft Cognitive Service",
    "slug": "vision",
    "privacy": "public",
    "sdkVersion": "23.0.0",
    "version": "1.1.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "ios": {
      "bundleIdentifier": "com.fanstek.vision",
      "buildNumber": "1010022",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.fanstek.vision",
      "versionCode": 1010022,
      "permissions": [
        "CAMERA"
      ]
    },
    "extra": {
      "semver": "1.1.0-beta.22",
      "showHintIdleSeconds": 5,
      "showInterstitialCalls": 10,
      "limitedAccessCalls": 3
    }
  }
}
