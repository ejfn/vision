{
  "expo": {
    "name": "AiVision",
    "description": "An AI Vision app powered by Microsoft Cognitive Service",
    "slug": "vision",
    "privacy": "public",
    "sdkVersion": "23.0.0",
    "version": "1.2.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "ios": {
      "bundleIdentifier": "com.fanstek.vision",
      "buildNumber": "1020004",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Please allow AiVision to use your camera to take photos for cognitive analysis.",
        "NSPhotoLibraryUsageDescription": "Pleas allow AiVision to access your photos for cognitive analysis.",
        "NSPhotoLibraryAddUsageDescription": "Please allow AiVision to save photos."
      }
    },
    "android": {
      "package": "com.fanstek.vision",
      "versionCode": 1020004,
      "permissions": [
        "CAMERA"
      ]
    },
    "extra": {
      "semver": "1.2.0-beta.4",
      "showHintIdleSeconds": 2,
      "showInterstitialCalls": 10,
      "limitedAccessCalls": 5,
      "adMobSettings": {
        "banners": [
          {
          },
          {
          }
        ],
        "interstitials": [
          {
          }
        ],
        "testDeviceIds": [
        ]
      },
      "geoAzureLocationMap": {
        "AF": "northeurope",
        "AN": "australiaeast",
        "AS": "southeastasia",
        "EU": "northeurope",
        "NA": "westus",
        "OC": "australiaeast",
        "SA": "westus"
      },
      "faceApiKeys": [
        {
          "location": "westus"
        },
        {
          "location": "southeastasia"
        },
        {
          "location": "australiaeast"
        },
        {
          "location": "northeurope"
        }
      ],
      "emotionApiKeys": [
        {
          "location": "westus"
        }
      ],
      "visionApiKeys": [
        {
          "location": "westus"
        },
        {
          "location": "australiaeast"
        },
        {
          "location": "southeastasia"
        },
        {
          "location": "northeurope"
        }
      ]
    }
  }
}
